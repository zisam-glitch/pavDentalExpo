{
  "expo": {
    "name": "pavdental",
    "slug": "pavdental",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pavdental",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.pavdental.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access for video consultations with your dentist",
        "NSMicrophoneUsageDescription": "This app requires microphone access for video consultations with your dentist",
        "UIBackgroundModes": ["audio", "voip"]
      }
    },
    "extra": {
      "SUPABASE_URL": "https://cpxsefyfabotgkcnauhn.supabase.co",
      "SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImNweHNlZnlmYWJvdGdrY25hdWhuIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjU0MzMzNzAsImV4cCI6MjA4MTAwOTM3MH0.TYPV-3W-yZSdYm1vcAQ4euEkaccmwspLuGKkJLuuefs",
      "router": {},
      "eas": {
        "projectId": "0e6bc9c9-8712-4c8e-872f-f8c7026cfa73"
      }
    },
    "android": {
      "package": "com.pavdental.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_CONNECT"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.pavdental",
          "enableGooglePay": true
        }
      ],
      [
        "@stream-io/video-react-native-sdk",
        {
          "enableScreenshare": false
        }
      ],
      "@stream-io/react-native-webrtc"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
